{
  "name": "koatest",
  "version": "1.0.0",
  "description": "testing koa",
  "main": "index.js",
  "scripts": {
    "build": "parcel build src/index.html",
    "back": "nodemon",
    "front": "parcel -p 4000 watch src/index.html --open --no-cache",
    "all": "concurrently \"parcel -p 4000 watch src/index.html --open --no-cache\" \"nodemon\"",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Synacten/Koatest.git"
  },
  "keywords": [
    "koa"
  ],
  "author": "Synacten",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Synacten/Koatest/issues"
  },
  "homepage": "https://github.com/Synacten/Koatest",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-react": "^7.0.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.4",
    "parcel-bundler": "^1.10.3"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static-server": "^1.3.4",
    "mysql2": "^1.6.4",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "sequelize": "^4.41.2",
    "styled-components": "^4.1.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
